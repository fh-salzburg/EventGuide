<%= render 'menu', { :permission => @permission } %>
<div id="left_column">
	<%= render 'eventlist' %>
</div>
<div id="right_column">
	<h1>Rangliste</h1>
	<table id="ranking">
	<tr>
		<td>Rang</td>
		<td>Name</td>
		<td>Sterne</td>
	</tr>
		<%
		@visitors.each do |visitor| %>
			<tr>
				<td><%= @rank %></td>
				<td><%= visitor.name %></td>
				<td><%= visitor.number_of_stars %> <%=image_tag("star.png") %></td>
			</tr>
			<% @rank += 1 %>
		<% end %>
	</table>
</div>
