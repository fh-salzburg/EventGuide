<div id="login">
<h3>Login</h3>

<%= form_tag sessions_path do %>
  <p>
    <%= label_tag :email %>
    <%= text_field_tag :email, params[:email], :size => "23" %>
  </p>
  <p>
    <%= label_tag :password %>
    <%= password_field_tag :password, "", :size => "23" %>
  </p>
  <p class="button"><%= submit_tag "Login", :class => "submit_tag" %></p>
<% end %>
</div>
